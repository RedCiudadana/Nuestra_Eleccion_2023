---
permalink: "/explicativos/index.html"
---

{% extends "src/base.njk" %}

{% block head %}
<title>Lab Electoral | Explicativos</title>
{% endblock %}

{% block content %}

        <!-- Slider
		============================================= -->
		<section id="slider" class="slider-element bg-color" style="background-image:url('/elementos/SLIDER-02.png')">
			<div class="container" >
				<div class="row h-100">
					<div class="col-md-6 align-self-left left pt-5 pb-0 py-lg-6 mb-0 my-lg-4" style="padding-bottom:0!important;margin-bottom:0!important">
						<img data-animate="fadeInLeft" data-delay="800" src="/elementos/NUBE_01.png">
					</div>
					<div class="col-md-6 align-self-right right pt-5 pb-0 py-lg-6 mb-0 my-lg-4" style="padding-bottom:0!important;margin-bottom:0!important;text-align:end">
						<img data-animate="fadeInLeft" data-delay="800" src="/elementos/NUBE_03.png">
					</div>
					
					<div class="align-self-center center  pb-0  mb-0 my-lg-4">
						<h1 class="text-white" style="font-size:30px; font-weight:700">Lorem Ipsum dolor sit amet, consectetuer</h1>
						<h3>Lorem Ipsum dolor sit amet,</h3>
					</div>


					<div id="nub1" class="col-md-6 align-self-left left" style="padding-bottom:0!important;margin-bottom:0!important">
						<img data-animate="fadeInRight" data-delay="800" style="margin: -80px 200px 0;" src="/elementos/NUBE_04.png">
					</div>
					<div id="nub2" class="col-md-6 align-self-right right " style="padding-bottom:0!important;margin-bottom:0!important;text-align:end">
						<img data-animate="fadeInRight" data-delay="800" style="margin: -200px 60px 0;" src="/elementos/NUBE_02.png">
					</div>

					<style>

					#nube1 {
						animation-duration: 5s;
						//animation-iteration-count:infinite;
						animation-name: slidein;
					}

						@keyframes slidein {
						from {
							margin-left: -50%;
						}

						to {
							margin-left: 0%;

						}
					}

					#nube2 {
						animation-duration: 9s;
						//animation-iteration-count:infinite;
						animation-name: slidein3;
					}

						@keyframes slidein3 {
						from {
							margin-left: 150%;
						}

						to {
							margin-left: 0%;

						}
					}

					#nube3 {
						animation-duration: 5s;
						//animation-iteration-count:infinite;
						animation-name: slidein2;
					}

						@keyframes slidein2 {
						from {
							margin-left: -100%;
						}

						to {
							margin-left: 200px;

						}
					}

					#nube4 {
						animation-duration: 8s;
						//animation-iteration-count:infinite;
						animation-name: slidein4;
					}

						@keyframes slidein4 {
						from {
							margin-left: 200%;
						}

						to {
							margin-left: 200px;

						}
					}

					</style>
				</div>
			</div>
			<div class="w-100">
				<img src="/elementos/SLIDER-01.png">
			</div>
		</section>

		<!-- Content
		============================================= -->
		<section id="content" style="background:#073350">

            <div class="content-wrap">
				<div class="container clearfix">
                    <h1 class="text-white" style="font-size:30px; font-weight:700">Explicativos</h1>
                    <br>
                    <br>
                    <div class="row">
                        <div class="col-md-4 col-lg-4 col-sm-12" style="border-right:#3385C7 0.2px solid">
							{% for explicativo in collections.explicativos | reverse | limit(4)%}
                            <a class="link_explicativo" href="{{explicativo.url}}" ><div class="d-flex" >
                                <h6 class="text-white link_explicativo" style="margin:0!important">{{loop.index}}. &nbsp {{explicativo.data.title}}</h6>
                            </div></a>
							{% endfor %}
                        </div>
						<div class="col-md-4 col-lg-4 col-sm-12" style="border-right:#3385C7 0.2px solid">
							{% for explicativo in collections.explicativos | reverse | limitPart(4,8)%}
                            <a class="link_explicativo" href="{{explicativo.url}}" ><div class="d-flex" >
                                <h6 class="text-white link_explicativo" style="margin:0!important">{% for explicativo2 in collections.explicativos | reverse%}{%if explicativo2 === explicativo %}{{loop.index}}{%endif%}{% endfor %}. &nbsp {{explicativo.data.title}}</h6>
								
                            </div></a>
							{% endfor %}
                        </div>
						<div class="col-md-4 col-lg-4 col-sm-12">
							{% for explicativo in collections.explicativos | reverse | limitPart(9,12)%}
                            <a class="link_explicativo" href="{{explicativo.url}}" ><div class="d-flex" >
                                <h6 class="text-white link_explicativo" style="margin:0!important">{% for explicativo2 in collections.explicativos | reverse%}{%if explicativo2 === explicativo %}{{loop.index}}{%endif%}{% endfor %}. &nbsp {{explicativo.data.title}}</h6>
                            </div></a>
							{% endfor %}
                        </div>

                    </div>
                </div>
            </div>

		</section><!-- #content end -->

{% endblock %}