{% extends "src/base.njk" %}

{% block head %}
<title>Nuestra Elección | Resultados</title>
{% endblock %}

{% block content %}

        <!-- Content
		============================================= -->
		<section id="content">
			<div class="content-wrap">

                <!-- event-area-start -->
                <div class="event-area pt-60 pb-130">
                    <div class="container">

                        <div class="container clearfix">
                            <div class="heading-block center">

                                <img style="margin:2% auto;padding:2% 45%"  src="/elementos/NUESTRA ELECCION-08.png" alt="Canvas Logo">

                                <h2>Resultados</h2>
                                <!--span>Built with passion &amp; intuitiveness in mind. Canvas is a masterclass piece of work presented to you.</span-->
                            </div>
                        </div>

                        <div class="row d-flex justify-content-center">
                            <div class="col-lg-12">
                                <div class="row">

                                <div class="col-md-4 col-lg-4 col-sm-12  my-4">
                                
                                </div>

                                <div class="d-flex justify-content-center">
                                    {% if pagination.href.previous %}
                                    <div class="mx-4">
                                        {% if pagination.href.previous %}<a href="{{ pagination.href.previous }}">← Iniciativas más recientes</a>{% else %}←  Nuevos blog{% endif %}
                                    </div>
                                    {% endif %}
                                    {% if pagination.href.next %}
                                    <div class="mx-4">
                                        {% if pagination.href.next %}<a href="{{ pagination.href.next }}">Iniciativas anteriores →</a>{% else %}Blog anteriores →{% endif %}
                                    </div>
                                    {% endif %}
                                </div>
                            </div>
                        </div>

                        

                        

                    </div>
                </div>



                </div>
            </div>
            <!-- event-area-end -->

			</div>
		</section><!-- #content end -->

{% endblock %}